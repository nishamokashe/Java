
//WAP to calculate Percentage of Student
class Four
{
    double om, tm, per;
    void getdata(double o, double t)
    {
        om=o;
        tm=t;
    }
    void calculate()
    {
        per = ((om*100)/tm);
        System.out.println("\n\n Percentage of student is"+per);

    }
}
class Check_Area
{   
    public static void main (String args[])
    {
        Four o = new Four();
        o.getdata(433, 500);
        o.calculate();
    }
}